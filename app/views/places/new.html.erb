<div class="container">
<h1 class="text-primary">New Place</h1>

<%= form_with model: @place do |form| %>
  <p>
    <%= form.label "name" , class:"form-label" %>
    <%= form.text_field "name", class: "form-control"  %>
  </p>
  <p>
    <%= form.submit "Create Place", class: "btn btn-success" %>
  </p>
<% end %>
</div>